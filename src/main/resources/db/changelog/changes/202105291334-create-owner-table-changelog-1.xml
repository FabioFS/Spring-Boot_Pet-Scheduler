            </column>
        </createTable>
        <addAutoIncrement tableName="owner" columnName="id" columnDataType="bigint" startWith="1" incrementBy="1" />  
        <createSequence sequenceName="hibernate_sequence" incrementBy="1" startValue="1" />
        
    </changeSet>
